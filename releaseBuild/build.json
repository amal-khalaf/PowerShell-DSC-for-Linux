        "BuildDockerOptions": [
            "--cap-add",
            "SYS_ADMIN",
            "--cap-add",
            "MKNOD",
            "--device=/dev/fuse",
            "--security-opt",
            "apparmor:unconfined"
        ],
        "DockerFile": "./releaseBuild/Images/microsoft_dsc_ubuntu16.04/Dockerfile",
        "DockerImageName": "ubunutu-16-04",
        "BinaryBucket": "release"
    }
  ]
